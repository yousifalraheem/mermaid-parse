root = true

[*.js]
indent_style = space
indent_size = 2
insert_final_newline = true
trim_trailing_whitespace = true
max_line_length = 120
quote_style = double
ij_javascript_force_quote_style = true
ij_typescript_force_quote_style = true

[*.html]
indent_style = space
indent_size = 2
quote_style = double
ij_html_quote_style = double
